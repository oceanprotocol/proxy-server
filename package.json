    "@typescript-eslint/parser": "^5.42.0",
    "@vercel/node": "^2.8.2",
    "axios": "^1.2.2",
    "chai": "^4.3.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.1",
    "pretty-quick": "^3.1.3",
    "test-listen": "^1.1.0",
    "ts-mocha": "^10.0.0",
